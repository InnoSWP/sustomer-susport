{
  "compilerOptions": {
		"outDir": "./flask_server/static/js",
		"rootDir": "./ts-modules",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es2015",
    "types": [],
    "lib": ["dom", "es5", "scripthost", "es2015"]
  },
  "exclude": ["node_modules"]
}
